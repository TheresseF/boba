  <body>

    <!-- upper design to homepage -->
      
    <div class="Bgupper">
      <img
        class="beverageslogo"
        alt=""
        src="/beverages-2-4@2x.png"
        id="beverages24"
      />
      <div class="bobasticlink" id="bOBASTICText">
        <b>BOBASTIC</b>
      </div>
      <div class="tab2">
      <b class="check-out">Check-out</b>
      </div>
      <b class="order">Order</b>
      <b class="payment1">Payment</b>
    </div>

    <!-- CheckOut area -->
    <div class="CheckOut">
      <div class="picture">
        <img class="logo" alt="" src="/beverages-2-31@2x.png" />
      </div>
      <b class="List">Order List</b> 
      <div class="CheckList"> 
        <table>
          <tr>
            <th class="format"> Product</th>
            <th class="format1"> Size</th>
            <th class="format1"> Quantity</th>
            <th class="format1"> Per Unit</th>
            <th class="format1"> Price</th>
          </tr>
          <tr>
            <td class="format2"> Chocolate</td>
            <td class="format3"> S</td>
            <td class="format3"> 1</td>
            <td class="format3"> 12</td>
            <td class="format3"> 14336</td>
          </tr>
        </table>
      </div>
      <div class="AllTotal">Total:</div>
      <div class="totalBox">
        <p class="total" id="inputPrice">14336</p>
      </div>
      <div>
        <p class="bobac">BobaCard:</p>
        <div class="bobac2" id="NumberInput">09** *** **49</div>
        <p class="bobac1">Points Receive :</p>
        <div class="bobac3" id="PInput">1.03</div>
      </div>
      <button class="exitbox" id="Exitbutton">
        <b class="exit">Exit</b>
      </button>
    </div>

